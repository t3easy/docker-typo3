{
    "name": "t3easy/typo3-bootcamp",
    "type": "project",
    "description": "Environment to develop and run TYPO3 in Docker containers",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Jan Kiesewetter",
            "email": "jan@t3easy.de"
        }
    ],
    "require": {
        "helhum/typo3-console": "^5.8",
        "helhum/typo3-secure-web": "^0.3",
        "typo3/cms-about": "^9.5",
        "typo3/cms-adminpanel": "^9.5",
        "typo3/cms-belog": "^9.5",
        "typo3/cms-beuser": "^9.5",
        "typo3/cms-felogin": "^9.5",
        "typo3/cms-fluid-styled-content": "^9.5",
        "typo3/cms-form": "^9.5",
        "typo3/cms-impexp": "^9.5",
        "typo3/cms-info": "^9.5",
        "typo3/cms-redirects": "^9.5",
        "typo3/cms-reports": "^9.5",
        "typo3/cms-rte-ckeditor": "^9.5",
        "typo3/cms-seo": "^9.5",
        "typo3/cms-setup": "^9.5",
        "typo3/cms-sys-note": "^9.5",
        "typo3/cms-t3editor": "^9.5",
        "typo3/cms-tstemplate": "^9.5",
        "typo3/cms-viewpage": "^9.5",
        "typo3/minimal": "^9.5"
    },
    "require-dev": {
        "typo3/cms-lowlevel": "^9.5"
    },
    "config": {
        "allow-plugins": {
            "helhum/typo3-console-plugin": true,
            "typo3/class-alias-loader": true,
            "typo3/cms-composer-installers": true
        },
        "platform": {
            "php": "7.4.30"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "9.5.x-dev"
        },
        "typo3/cms": {
            "root-dir": "private",
            "web-dir": "public"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "@typo3-cms-scripts"
        ],
        "typo3-cms-scripts": [
            "typo3cms install:fixfolderstructure",
            "typo3cms install:generatepackagestates"
        ]
    }
}
