{
    "name": "t3easy/typo3-bootcamp",
    "type": "project",
    "description": "Environment to develop and run TYPO3 in Docker containers",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Jan Kiesewetter",
            "email": "jan@t3easy.de"
        }
    ],
    "require": {
        "helhum/typo3-console": "^6.4.0",
        "helhum/typo3-secure-web": "^0.3.1",
        "typo3/cms-backend": "dev-master as 11.0.99",
        "typo3/cms-belog": "dev-master as 11.0.99",
        "typo3/cms-beuser": "dev-master as 11.0.99",
        "typo3/cms-core": "dev-master as 11.0.99",
        "typo3/cms-dashboard": "dev-master as 11.0.99",
        "typo3/cms-extbase": "dev-master as 11.0.99",
        "typo3/cms-extensionmanager": "dev-master as 11.0.99",
        "typo3/cms-felogin": "dev-master as 11.0.99",
        "typo3/cms-filelist": "dev-master as 11.0.99",
        "typo3/cms-fluid": "dev-master as 11.0.99",
        "typo3/cms-fluid-styled-content": "dev-master as 11.0.99",
        "typo3/cms-form": "dev-master as 11.0.99",
        "typo3/cms-frontend": "dev-master as 11.0.99",
        "typo3/cms-impexp": "dev-master as 11.0.99",
        "typo3/cms-info": "dev-master as 11.0.99",
        "typo3/cms-install": "dev-master as 11.0.99",
        "typo3/cms-recordlist": "dev-master as 11.0.99",
        "typo3/cms-redirects": "dev-master as 11.0.99",
        "typo3/cms-rte-ckeditor": "dev-master as 11.0.99",
        "typo3/cms-scheduler": "dev-master as 11.0.99",
        "typo3/cms-seo": "dev-master as 11.0.99",
        "typo3/cms-setup": "dev-master as 11.0.99",
        "typo3/cms-sys-note": "dev-master as 11.0.99",
        "typo3/cms-t3editor": "dev-master as 11.0.99",
        "typo3/cms-tstemplate": "dev-master as 11.0.99",
        "typo3/cms-viewpage": "dev-master as 11.0.99"
    },
    "require-dev": {
        "typo3/cms-adminpanel": "dev-master as 11.0.99",
        "typo3/cms-lowlevel": "dev-master as 11.0.99",
        "typo3/cms-reports": "dev-master as 11.0.99"
    },
    "config": {
        "platform": {
            "php": "7.4.15"
        },
        "sort-packages": true
    },
    "extra": {
        "typo3/cms": {
            "root-dir": "private",
            "web-dir": "public"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "@typo3-cms-scripts"
        ],
        "typo3-cms-scripts": [
            "typo3cms install:fixfolderstructure",
            "typo3cms install:generatepackagestates"
        ]
    }
}
